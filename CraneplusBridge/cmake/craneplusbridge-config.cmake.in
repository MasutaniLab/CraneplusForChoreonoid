# CraneplusBridge CMake config file
#
# This file sets the following variables:
# CraneplusBridge_FOUND - Always TRUE.
# CraneplusBridge_INCLUDE_DIRS - Directories containing the CraneplusBridge include files.
# CraneplusBridge_IDL_DIRS - Directories containing the CraneplusBridge IDL files.
# CraneplusBridge_LIBRARIES - Libraries needed to use CraneplusBridge.
# CraneplusBridge_DEFINITIONS - Compiler flags for CraneplusBridge.
# CraneplusBridge_VERSION - The version of CraneplusBridge found.
# CraneplusBridge_VERSION_MAJOR - The major version of CraneplusBridge found.
# CraneplusBridge_VERSION_MINOR - The minor version of CraneplusBridge found.
# CraneplusBridge_VERSION_REVISION - The revision version of CraneplusBridge found.
# CraneplusBridge_VERSION_CANDIDATE - The candidate version of CraneplusBridge found.

message(STATUS "Found CraneplusBridge-@PROJECT_VERSION@")
set(CraneplusBridge_FOUND TRUE)

find_package(<dependency> REQUIRED)

#set(CraneplusBridge_INCLUDE_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@"
#    ${<dependency>_INCLUDE_DIRS}
#    )
#
#set(CraneplusBridge_IDL_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@/idl")
set(CraneplusBridge_INCLUDE_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@"
    ${<dependency>_INCLUDE_DIRS}
    )
set(CraneplusBridge_IDL_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@/idl")


if(WIN32)
    set(CraneplusBridge_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_STATIC_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
else(WIN32)
    set(CraneplusBridge_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_SHARED_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
endif(WIN32)

set(CraneplusBridge_DEFINITIONS ${<dependency>_DEFINITIONS})

set(CraneplusBridge_VERSION @PROJECT_VERSION@)
set(CraneplusBridge_VERSION_MAJOR @PROJECT_VERSION_MAJOR@)
set(CraneplusBridge_VERSION_MINOR @PROJECT_VERSION_MINOR@)
set(CraneplusBridge_VERSION_REVISION @PROJECT_VERSION_REVISION@)
set(CraneplusBridge_VERSION_CANDIDATE @PROJECT_VERSION_CANDIDATE@)

