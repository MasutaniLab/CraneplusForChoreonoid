# DynamixelSim CMake config file
#
# This file sets the following variables:
# DynamixelSim_FOUND - Always TRUE.
# DynamixelSim_INCLUDE_DIRS - Directories containing the DynamixelSim include files.
# DynamixelSim_IDL_DIRS - Directories containing the DynamixelSim IDL files.
# DynamixelSim_LIBRARIES - Libraries needed to use DynamixelSim.
# DynamixelSim_DEFINITIONS - Compiler flags for DynamixelSim.
# DynamixelSim_VERSION - The version of DynamixelSim found.
# DynamixelSim_VERSION_MAJOR - The major version of DynamixelSim found.
# DynamixelSim_VERSION_MINOR - The minor version of DynamixelSim found.
# DynamixelSim_VERSION_REVISION - The revision version of DynamixelSim found.
# DynamixelSim_VERSION_CANDIDATE - The candidate version of DynamixelSim found.

message(STATUS "Found DynamixelSim-@PROJECT_VERSION@")
set(DynamixelSim_FOUND TRUE)

find_package(<dependency> REQUIRED)

#set(DynamixelSim_INCLUDE_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@"
#    ${<dependency>_INCLUDE_DIRS}
#    )
#
#set(DynamixelSim_IDL_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@/idl")
set(DynamixelSim_INCLUDE_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@"
    ${<dependency>_INCLUDE_DIRS}
    )
set(DynamixelSim_IDL_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@/idl")


if(WIN32)
    set(DynamixelSim_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_STATIC_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
else(WIN32)
    set(DynamixelSim_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_SHARED_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
endif(WIN32)

set(DynamixelSim_DEFINITIONS ${<dependency>_DEFINITIONS})

set(DynamixelSim_VERSION @PROJECT_VERSION@)
set(DynamixelSim_VERSION_MAJOR @PROJECT_VERSION_MAJOR@)
set(DynamixelSim_VERSION_MINOR @PROJECT_VERSION_MINOR@)
set(DynamixelSim_VERSION_REVISION @PROJECT_VERSION_REVISION@)
set(DynamixelSim_VERSION_CANDIDATE @PROJECT_VERSION_CANDIDATE@)

